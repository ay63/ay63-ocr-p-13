{
  "/api": {
    "target": "http://localhost:8080", 
    "secure": false,
    "changeOrigin": true
  },
  "/chats": {
    "target": "http://localhost:8080",
    "secure": false,
    "pathRewrite": { "^/chats": "/api/chats" }
  },
"/websocket": {
  "target": "http://localhost:8080",
  "ws": true,
  "secure": false,
  "pathRewrite": { "^/websocket": "/api/websocket" }
},
"/websocket/info": {
  "target": "http://localhost:8080",
  "secure": false,
  "pathRewrite": { "^/websocket/info": "/api/websocket/info" }
}
}
